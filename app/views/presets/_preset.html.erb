<%= content_tag_for :div, preset, class: 'span6' do %>
  <h3><%= preset.name %><%= " (weight: #{preset.weight})" unless preset.weight.blank? %></h3>
  <pre><%= preset.parameters %></pre>
  <%= link_to 'Edit', edit_preset_path(preset), class: 'btn btn-mini' %> 
  <%= link_to 'Delete', api_preset_path(preset), class: 'btn btn-mini', method: :delete, confirm: t('confirm.delete', :model_name => 'preset') %>
<% end %>
