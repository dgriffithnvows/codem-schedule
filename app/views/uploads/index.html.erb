<% title 'Uploads' %>

<% for upload in @uploads %>
  <div class="gallery">
    <p>
      <%= upload.name %>
      <%= link_to "edit", edit_upload_path(upload) %>
      <%= link_to "remove", upload, :confirm => "Are you sure you want to delete this webinar", :method => :delete %>
    </p>
  </div>
<% end %>
